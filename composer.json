{
    "name": "uladzislaumakshou/nova-chartjs",
    "description": "A Simple Dashboard Chart in Laravel Nova using Chart JS. Starting create your own dashboard with Chart JS Integration can save your time and help you maintain consistency across standard elements such as Bar, Stacked, Line, Area, Doughnut and Pie Chart.",
    "keywords": [
        "laravel",
        "nova",
        "stacked chart",
        "chart js"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "NovaChartJs\\ChartJsIntegration\\": "src/",
            "NovaChartJs\\ChartJsIntegration\\Api\\": "src/api"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaChartJs\\ChartJsIntegration\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
